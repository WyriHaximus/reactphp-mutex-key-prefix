{
    "name": "wyrihaximus/react-mutex-key-prefix",
    "description": "ReactPHP Mutex Key Prefix",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "react/promise": "^2.8",
        "thecodingmachine/safe": "^1.3",
        "wyrihaximus/react-mutex-contracts": "^2"
    },
    "require-dev": {
        "wyrihaximus/react-mutex": "^3.1",
        "wyrihaximus/react-mutex-test-utilities": "^2"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Mutex\\KeyPrefix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\React\\Mutex\\KeyPrefix\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
